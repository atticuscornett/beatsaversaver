{
  "name": "beatsaversaver",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "description": "An unofficial, mod-free browser and installer for levels from Beat Saver",
  "author": "Atticus Cornett <64325242+atticuscornett@users.noreply.github.com>",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "rollup -c && electron ."
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "electron": "^31.2.1",
    "rollup": "^4.19.1",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.5",
    "svelte": "^4.0.0"
  },
  "dependencies": {
    "adm-zip": "^0.5.14",
    "electron-builder": "^24.13.3",
    "node-persist": "^4.0.2",
    "sirv-cli": "^2.0.0"
  },
  "build": {
    "appId": "io.atticusc.beatsaversaver",
    "productName": "beatsaversaver",
    "icon": "icon.png",
    "win": {
      "target": "nsis",
      "icon": "icon.png",
      "publish": [
        "github"
      ]
    },
    "linux": {
      "target": [
        {
          "target": "deb",
          "arch": [
            "x64",
            "armv7l"
          ]
        },
        "tar.gz",
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "icon.png"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "icon.png"
    }
  }
}
